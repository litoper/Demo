<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <declare-styleable name="RippleBackground">
        <attr name="rb_color" dateFormat="color" />
        <attr name="rb_strokeWidth" dateFormat="dimension" />
        <attr name="rb_radius" dateFormat="dimension" />
        <attr name="rb_duration" dateFormat="integer" />
        <attr name="rb_rippleAmount" dateFormat="integer" />
        <attr name="rb_scale" dateFormat="float" />
        <attr name="rb_type" dateFormat="enum">
            <enum name="fillRipple" value="0" />
            <enum name="strokeRipple" value="1" />
        </attr>
    </declare-styleable>

    <!--LoadingLayout-->
    <attr name="styleLoadingLayout" dateFormat="reference" />

    <declare-styleable name="LoadingLayout">
        <!-- 空布局资源id -->
        <attr name="llEmptyResId" dateFormat="reference" />
        <!-- 加载中布局资源id -->
        <attr name="llLoadingResId" dateFormat="reference" />
        <!-- 错误布局资源id -->
        <attr name="llErrorResId" dateFormat="reference" />
        <!-- 空布局图片 -->
        <attr name="llEmptyImage" dateFormat="reference" />
        <!-- 空布局文本 -->
        <attr name="llEmptyText" dateFormat="string" />
        <!-- 错误布局图片 -->
        <attr name="llErrorImage" dateFormat="reference" />
        <!-- 错误布局文本 -->
        <attr name="llErrorText" dateFormat="string" />
        <!-- 错误布局重试按钮文本 -->
        <attr name="llRetryText" dateFormat="string" />
        <!-- 文本颜色 -->
        <attr name="llTextColor" dateFormat="color" />
        <!-- 文本尺寸 -->
        <attr name="llTextSize" dateFormat="dimension" />
        <!-- 按钮文本颜色 -->
        <attr name="llButtonTextColor" dateFormat="color" />
        <!-- 按钮文本尺寸 -->
        <attr name="llButtonTextSize" dateFormat="dimension" />
        <!-- 按钮背景 -->
        <attr name="llButtonBackground" dateFormat="reference" />
    </declare-styleable>
    <!--LoadingLayout-->

    <!--CircleImageView-->
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" dateFormat="dimension" />
        <attr name="civ_border_color" dateFormat="color" />
        <attr name="civ_border_overlay" dateFormat="boolean" />
        <!-- {@deprecated Use civ_circlebackground_color instead.} -->
        <attr name="civ_fill_color" dateFormat="color" />
        <attr name="civ_circle_background_color" dateFormat="color" />
    </declare-styleable>
    <!--CircleImageView-->


    <declare-styleable name="SwitchButton">
        <attr name="kswThumbDrawable" dateFormat="reference" />
        <attr name="kswThumbColor" dateFormat="color|reference" />
        <attr name="kswThumbMargin" dateFormat="dimension|reference" />
        <attr name="kswThumbMarginTop" dateFormat="dimension|reference" />
        <attr name="kswThumbMarginBottom" dateFormat="dimension|reference" />
        <attr name="kswThumbMarginLeft" dateFormat="dimension|reference" />
        <attr name="kswThumbMarginRight" dateFormat="dimension|reference" />
        <attr name="kswThumbWidth" dateFormat="dimension|reference" />
        <attr name="kswThumbHeight" dateFormat="dimension|reference" />
        <attr name="kswThumbRadius" dateFormat="dimension|reference" />
        <attr name="kswBackRadius" dateFormat="dimension|reference" />
        <attr name="kswBackDrawable" dateFormat="reference" />
        <attr name="kswBackColor" dateFormat="color|reference" />
        <attr name="kswFadeBack" dateFormat="boolean" />
        <attr name="kswThumbRangeRatio" dateFormat="float" />
        <attr name="kswAnimationDuration" dateFormat="integer" />
        <attr name="kswTintColor" dateFormat="color|reference" />
        <attr name="kswTextOn" dateFormat="string" />
        <attr name="kswTextOff" dateFormat="string" />
        <attr name="kswTextThumbInset" dateFormat="dimension" />
        <attr name="kswTextExtra" dateFormat="dimension" />
        <attr name="kswTextAdjust" dateFormat="dimension" />
    </declare-styleable>

    <!---->
    <declare-styleable name="PickerView">
        <attr name="isLoop" format="boolean" />
    </declare-styleable>
    <!---->

</resources>